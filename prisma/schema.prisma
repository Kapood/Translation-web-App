// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:/dev.db"
}
model Language {
  id        String @id @default(cuid())
  inEnglish String
  inPidgin  String
}